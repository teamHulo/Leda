        "958396e1-7844-4936-b161-a98bc72e521c": {
          "type": "text",
          "settings": {
            "max_width": 1000,
            "padding": 0,
            "padding_left": 0,
            "padding_right": 150,
            "item_bg_color": "",
            "item_text_color": "",
            "title": "Passion Becomes Reality",
            "richtext": "<p>Passionate about revolutionising plant-based hair care, the vision of LEDA Swiss was born. Scientific innovation, nature, luxury, and Switzerland continuously inspire the brand.<\/p>",
            "image_mob": "shopify:\/\/shop_images\/Rectangle_83.jpg",
            "line_title": "2019"
          }
        },
